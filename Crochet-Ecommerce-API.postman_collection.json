          "name": "Register User",
          "event": [
            {
              "listen": "test",
              "script": {
                "id": "d6a9df35-b95f-4c5e-bdee-12364813adcd",
                "exec": [
                  "const responseBodyJson = pm.response.json();",
                  "",
                  "pm.test('Status code is 200', function () {",
                  "  pm.response.to.have.status(200);",
                  "});",
                  "",
                  "pm.test('Response has user object', function () {",
                  "  pm.expect(responseBodyJson).to.have.property('user');",
                  "  pm.expect(responseBodyJson).to.have.property('message').that.equals('Registration successful');",
                  "});",
                  "",
                  "pm.test('User has required fields', function () {",
                  "  const user = responseBodyJson.user;",
                  "  pm.expect(user).to.have.property('id');",
                  "  pm.expect(user).to.have.property('email');",
                  "  pm.expect(user).to.have.property('name');",
                  "  pm.expect(user).to.have.property('role');",
                  "});",
                  "",
                  "pm.test('Password is NOT returned', function () {",
                  "  const user = responseBodyJson.user;",
                  "  pm.expect(user).to.not.have.property('password');",
                  "});",
                  "",
                  "// Save user ID for later requests",
                  "if (responseBodyJson.user && responseBodyJson.user.id) {",
                  "  pm.environment.set('userId', responseBodyJson.user.id);",
                  "}"
                ],
